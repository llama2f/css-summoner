    "sourceFile": "card/variants.css"
  },
  "card-outline": {
    "ruleText": ".card-outline {\n\t\tbackground-color: transparent;\n\t\tcolor: var(\n\t\t\t--component-color,\n\t\t\t#333\n\t\t); /* テキスト色はコンポーネントカラーに合わせる */\n\t\tborder-color: var(\n\t\t\t--component-border,\n\t\t\t#333\n\t\t); /* ボーダー色はコンポーネントカラー */\n\t}",
    "selector": ".card-outline",
    "sourceFile": "card/variants.css"
  },
  "card-ghost": {
    "ruleText": ".card-ghost {\n\t\tbackground-color: transparent;\n\t\tcolor: var(--component-color, #333);\n\t\tborder-color: transparent;\n\t}",
