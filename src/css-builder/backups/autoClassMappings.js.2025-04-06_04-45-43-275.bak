  },
  "heading-formal-underline": {
    "ruleText": ".heading-formal-underline {\n\t\t@apply heading-base;\n\t\tpadding-bottom: var(--heading-padding-y);\n\t\tborder-bottom: 2px solid var(--component-border, #333);\n\t\tcolor: var(--component-color, #333);\n\t}",
