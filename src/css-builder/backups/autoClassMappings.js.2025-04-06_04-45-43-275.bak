  "heading-formal-full": {
    "ruleText": ".heading-formal-full {\n\t\t@apply heading-base rounded;\n\t\tpadding: var(--heading-padding-y) var(--heading-padding-x);\n\t\tbackground-color: var(--component-color, #333);\n\t\tcolor: var(--component-text, #fff);\n\t\t/* ホバー/アクティブ時の色調整はカラーセレクタ側で行う */\n\t}",
