		"css-components": "node src/css-summoner/scripts/css-summoner-integration.js --components",
		"css-all": "node src/css-summoner/scripts/css-summoner-integration.js --all",
		"generate:handlers": "node src/css-summoner/scripts/generate-handler-manifest.js",
		"generate:astro": "node src/css-summoner/scripts/generate-astro.js",
		"generate:astro-force": "node src/css-summoner/scripts/generate-astro.js --force",
		"extract-jsx": "node src/css-summoner/scripts/extract-jsx.js src/css-summoner/templates/handlers/auto"
	},
