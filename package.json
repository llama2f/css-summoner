{
	"private": true,
	"name": "css-summoner",
	"version": "0.1.0",
	"description": "CSS Summoner",
	"type": "module",
	"scripts": {
		"dev": "npm run generate:handlers && astro dev --host --port 5000",
		"start": "astro dev",
		"build": "npm run generate:handlers && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint .",
		"check": "astro check",
		"map": "node src/css-summoner/scripts/simple-file-mapper.js",
